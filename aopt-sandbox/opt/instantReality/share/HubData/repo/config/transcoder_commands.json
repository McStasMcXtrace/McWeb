        {
            "name"              : "CAD2VIS Cad to VRML/OSB/OSG",
            "acceptedMimetypes" : ["application/catia"],
            "resultMimetype"    : "model/x-src",

            "transcoderParams" :
            {
              "ServiceClass" : "TranscoderService",
              "Parameters"   :
              {
                "type": "SecCmdBatch",
                "cmds":
                [
                    "wget -O ${INPUT_FULL_NAME} ${INPUT_URL}",
                    "cad2vis -i ${INPUT_FULL_NAME} -of osb -o ${INPUT_NAME}",
                    "aopt -i ${INPUT_NAME}.osb -F Scene:alphaGeometry(1),cacheopt(1) -Y nodeType(Scene),sharedSRCs(true),collapseGraph(true),encodingPreset(CMP1)"
                ]
              }
            },

            "webVISParams" :
            {
                "replyType"      : "path",
                "resultFilename" : "src0.src"
            }
